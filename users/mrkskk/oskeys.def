//  OSKEY NAME               WINDOWS KEYCODE                  MAC KEYCODE

//  Mac/windows keycode differences on danish iso.

    OSKEY(LCB,                   ALGR(KC_7),                    LSA(KC_8))     // {
    OSKEY(RCB,                   ALGR(KC_0),                    LSA(KC_9))      // }
    OSKEY(AT,                    ALGR(KC_2),                    LALT(QUOT))    // @
    OSKEY(BSLH,                  ALGR(LABK),                    LALT(SLSH))     // Backslash
    OSKEY(PIPE,                  ALGR(KC_EQL),                  LALT(KC_I))    // |
    OSKEY(EUR,                   ALGR(KC_5),                    S(KC_4))       // €
    OSKEY(USD,                   ALGR(KC_4),                    ALGR(S(KC_3))) // $
    OSKEY(GBP,                   ALGR(KC_3),                    A(KC_4))       // £

// Navigation 2 that differ between danish MAC keyboard and danish WIN keyboard

    OSKEY(NXTW,                  LCTL(KC_RIGHT),                LALT(KC_RIGHT))      // Cursor jump to Next Word
    OSKEY(PRVW,                  LCTL(KC_LEFT),                 LALT(KC_LEFT))       // Cursor jump to previous Word
    OSKEY(S_NXTW,                LCTL(S(KC_RIGHT)),             LALT(S(KC_RIGHT)))   // Cursor select next word
    OSKEY(S_PRVW,                LCTL(S(KC_LEFT)),              LALT(S(KC_LEFT)))    // Cursor select previous word
    OSKEY(S_UP_W,                LCTL(S(KC_UP)),                LALT(S(KC_UP)))      // Cursor select  word up
    OSKEY(S_DN_W,                LCTL(S(KC_DOWN)),              LALT(S(KC_DOWN)))    // Cursor select word down
    OSKEY(SCR_SHOT,              KC_PSCREEN,                    LGUI(S(KC_4)))       // Screenshot
    OSKEY(COPY,                  LCTL(KC_C),                    LGUI(KC_C))          // COPY
    OSKEY(PASTE,                 LCTL(KC_V),                    LGUI(KC_V))          // PASTE
    OSKEY(CUT,                   LCTL(KC_X),                    LGUI(KC_X))          // CUT
    OSKEY(UNDO,                  LCTL(KC_Z),                    LGUI(KC_Z))          // UNDO
    OSKEY(REDO,                  LCTL(KC_Y),                    LGUI(KC_Y))          // REDO
    OSKEY(FIND,                  KC_LGUI,                       LGUI(KC_SPC))       // Alfred/win menu
    OSKEY(BSPC_W,                LCTL(KC_BSPC),                 LALT(KC_BSPC))
    OSKEY(BSPC_S,                S(KC_HOME | KC_BSPC),          LGUI(KC_BSPC))
    OSKEY(DEL_W,                 LCTL(KC_DEL),                  LALT(KC_DEL))
    OSKEY(DEL_S,                 S(KC_END | KC_DEL),            LGUI(KC_DEL))
